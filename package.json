{
  "name": "nina",
  "version": "0.1.0",
  "private": true,
  "main": "public/electron.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "elec-dev": "concurrently \"BROWSER=none react-scripts start\" \"wait-on http://localhost:3000 && electron .\"",
    "package": "npm run build && electron-builder"
  },
  "dependencies": {
    "@types/jest": "24.0.15",
    "@types/node": "12.6.8",
    "@types/react": "16.8.23",
    "@types/react-dom": "16.8.4",
    "antd": "^3.20.5",
    "braft-editor": "^2.3.7",
    "electron-is-dev": "^1.1.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "3.0.1",
    "typescript": "3.5.3"
  },
  "devDependencies": {
    "concurrently": "^4.1.1",
    "electron": "^5.0.7",
    "electron-builder": "^21.1.1",
    "wait-on": "^3.3.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "homepage": "./",
  "build": {
    "appId": "com.xxx.app",
    "npmRebuild": true,
    "mac": {
      "category": "tools",
      "icon": "build/icon.png"
    },
    "win": {
      "icon": "build/icon.png",
      "target": "nsis"
    },
    "nsis": {
      "allowToChangeInstallationDirectory": true,
      "allowElevation": false,
      "createDesktopShortcut": true,
      "menuCategory": true,
      "oneClick": false
    },
    "files": [
      "build/**/*"
    ]
  }
}
